# Ditto SDK Configuration Template
# Copy this file to .env and fill in your actual values
# Get these values from https://portal.ditto.live

# Development Configuration
EXPO_PUBLIC_DITTO_APP_ID=your-app-id-here
EXPO_PUBLIC_DITTO_PLAYGROUND_TOKEN=your-playground-token-here

# Production Configuration (optional)
EXPO_PUBLIC_DITTO_PROD_APP_ID=your-prod-app-id-here
EXPO_PUBLIC_DITTO_WEBSOCKET_URL=wss://your-websocket-url-here
EXPO_PUBLIC_DITTO_AUTH_URL=https://your-auth-url-here

# Environment
NODE_ENV=development
